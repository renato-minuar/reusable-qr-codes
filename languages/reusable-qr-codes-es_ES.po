msgstr "Estado"

msgid "Destination"
msgstr "Destino"

msgid "Download"
msgstr "Descargar"

msgid "Active"
msgstr "Activo"

msgid "Inactive"
msgstr "Inactivo"

msgid "No destination set"
msgstr "Sin destino configurado"

